{
  "name": "dsrfa-web-application",
  "private": true,
  "version": "1.2.5",
  "type": "module",
  "description": "Davao-South Regional Football Association - Web Application for member management, events, and community engagement",
  "keywords": [
    "football",
    "association",
    "davao",
    "sports",
    "membership",
    "events",
    "react",
    "typescript",
    "tailwind"
  ],
  "author": "DSRFA Development Team",
  "license": "MIT",
  "homepage": "https://dsrfa.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/dsrfa/web-application.git"
  },
  "bugs": {
    "url": "https://github.com/dsrfa/web-application/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm-rf dist node_modules/.vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
